      "['sepal length (cm)', 'sepal width (cm)', 'petal length (cm)', 'petal width (cm)', 'target', 'species']\n"
     ]
    }
   ],
   "source": [
    "all_cols = list(df.columns.values)\n",
    "print(all_cols)\n",
    "features = all_cols[:4]\n",
    "target = all_cols[-1]\n",
    "# Separating out the features\n",
    "x = df.loc[:, features].values\n",
    "# Separating out the target\n",
    "y = df.loc[:,target].values\n",
    "# # Standardizing the features\n",
    "x = StandardScaler().fit_transform(x)\n",
    "# print(x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 0,
   "metadata": {
    "colab": {},
    "colab_type": "code",
    "id": "5ZEeBkNzNm3W"
   },
   "outputs": [],
   "source": [
    "from sklearn.decomposition import PCA\n",
    "pca = PCA(n_components=4)\n",
    "principalComponents = pca.fit_transform(x)\n",
    "firsttTwo_PC = principalComponents[:,:2]\n",
    "lastTwo_PC = principalComponents[:,2:]\n",
    "principalDf_1 = pd.DataFrame(data = firsttTwo_PC\n",
    "             , columns = ['Principal component 1', 'Principal component 2'])\n",
    "principalDf_2 = pd.DataFrame(data = lastTwo_PC, columns = ['Principal component 3', 'Principal component 4' ])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 0,
   "metadata": {
    "colab": {},
    "colab_type": "code",
    "id": "EsWGJupDRiXC"
   },
   "outputs": [],
   "source": [
    "def visualize_PC_1():\n",
    "  finalDf = pd.concat([principalDf_1, df[['species']]], axis = 1)\n",
    "  fig = plt.figure(figsize = (8,8))\n",
    "  ax = fig.add_subplot(1,1,1) \n",
    "  ax.set_xlabel('Principal Component 1', fontsize = 15)\n",
    "  ax.set_ylabel('Principal Component 2', fontsize = 15)\n",
    "  ax.set_title('2-Component PCA', fontsize = 20)\n",
    "  targets = ['setosa', 'versicolor', 'virginica']\n",
    "  colors = ['r', 'g', 'b']\n",
    "  for target, color in zip(targets,colors):\n",
    "      indicesToKeep = finalDf['species'] == target\n",
    "      ax.scatter(finalDf.loc[indicesToKeep, 'Principal component 1']\n",
    "                , finalDf.loc[indicesToKeep, 'Principal component 2']\n",
    "                , c = color\n",
    "                , s = 50)\n",
    "  ax.legend(targets)\n",
    "  ax.grid()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 0,
   "metadata": {
    "colab": {},
    "colab_type": "code",
    "id": "gKI4hUQPTpFD"
   },
   "outputs": [],
   "source": [
    "def visualize_PC_2():\n",
    "  finalDf_2 = pd.concat([principalDf_2, df[['species']]], axis = 1)\n",
    "  fig = plt.figure(figsize = (8,8))\n",
    "  ax = fig.add_subplot(1,1,1) \n",
    "  ax.set_xlabel('Principal Component 3', fontsize = 15)\n",
    "  ax.set_ylabel('Principal Component 4', fontsize = 15)\n",
    "  ax.set_title('2-Component PCA', fontsize = 20)\n",
    "  targets = ['setosa', 'versicolor', 'virginica']\n",
    "  colors = ['r', 'g', 'b']\n",
    "  for target, color in zip(targets,colors):\n",
    "      indicesToKeep = finalDf_2['species'] == target\n",
    "      ax.scatter(finalDf_2.loc[indicesToKeep, 'Principal component 3']\n",
    "                , finalDf_2.loc[indicesToKeep, 'Principal component 4']\n",
    "                , c = color\n",
    "                , s = 50)\n",
    "  ax.legend(targets)\n",
    "  ax.grid()\n"
   ]
