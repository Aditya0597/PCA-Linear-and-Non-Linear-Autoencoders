   "source": [
    "### Information contained by each component"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 0,
   "metadata": {
    "colab": {},
    "colab_type": "code",
    "id": "jZ8rbMBvRnlc"
   },
   "outputs": [],
   "source": [
    "def scree_plot():\n",
    "  variance_list = list(pca.explained_variance_ratio_)\n",
    "  for i in range(len(variance_list)):\n",
    "    print(\"Variance information contained by PC {}: {}%\".format(i, ((variance_list[i]*100).round(2))))\n",
    "  plt.plot(np.cumsum(pca.explained_variance_ratio_))\n",
    "  plt.xlabel('Number of Components')\n",
    "  plt.ylabel('Cumulative Explained Variance')\n",
    "  plt.axvline(x=1, linewidth=1, color='r', alpha=0.5)\n",
    "  plt.axvline(x=2, linewidth=1, color='r', alpha=0.5)\n",
    "  plt.axvline(x=3, linewidth=1, color='r', alpha=0.5)\n",
    "  plt.axvline(x=0, linewidth=1, color='r', alpha=0.5)\n",
    "  plt.axhline(y=0.7296,linewidth=1, color='r', alpha=0.5)\n",
    "  plt.axhline(y=0.9581,linewidth=1, color='r', alpha=0.5)\n",
    "  plt.axhline(y=0.9948,linewidth=1, color='r', alpha=0.5)\n",
    "  plt.axhline(y=1,linewidth=1, color='r', alpha=0.5)\n",
    "  plt.xticks([0,1,2,3])"
   ]
  },
